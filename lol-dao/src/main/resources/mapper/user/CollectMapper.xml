<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.nf.lol.user.dao.CollectDao">
    <resultMap id="collectMap" type="edu.nf.lol.user.entity.Collect">
        <id property="collectId" column="collect_id"/>
        <result property="userId" column="user_id"/>
        <result property="productId" column="product_id"/>
    </resultMap>


    <insert id="addCollect">
        insert into collect_info(user_id,product_id) values(#{param1},#{param2})
    </insert>


</mapper>